  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "expect": "^26.1.0",
    "express": "^4.17.1",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "sequelize-auto-migrations": "^1.0.3",
    "sequelize-cli": "^5.5.1",
    "sinon": "^9.0.2",
    "supertest": "^4.0.2"
  }
}
